<div class="row">
  <div class="container text-center">
    <div class="featurette">
       <div class="featurette-inner text-center">
          <form role="form" class="search" ng-submit="search(inputText)">
             <h3 class="no-margin-top h1">Search for skins or weapons</h3>
             <div class="input-group input-group-lg">
                <input list="weapon-search-datalist" type="search" ng-model="inputText" placeholder="Ex: AK-47" class="form-control">
                <datalist id="weapon-search-datalist">
                  <select ng-repeat="wep in weapons">
                    <option value="{{wep.name}}"></option>
                    <option ng-repeat="skin in wep.skins" value="{{skin}}"></option>
                  </select>
                </datalist>
                <span class="input-group-btn">
                  <button class="btn btn-success" type="submit" value="Submit">Search</button>
                </span>
             </div>
          </form>
       </div>
    </div>
  </div>
</div>

<div class="row">
  <div class="container">
    <ul class="list-group">
      <li
        ng-repeat="result in results"
        class="list-group-item">
        {{result.name}}
      </li>
    </ul>
  </div>
</div>
