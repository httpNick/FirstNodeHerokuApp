<div class="jumbotron">
  <div class="featurette">
     <div class="featurette-inner text-center">
        <form role="form" class="search" ng-submit="search(inputText)">
           <h3 class="no-margin-top h1">Find a CS:GO weapon</h3>
           <div class="input-group input-group-lg">
              <input list="weapon-search-datalist" type="search" ng-model="inputText" placeholder="search" class="form-control">
              <datalist id="weapon-search-datalist">
                <option ng-repeat="wep in weapons" value="{{wep.name}}"></option>
              </datalist>
              <span class="input-group-btn">
                <button class="btn btn-danger" type="submit" value="Submit">Search</button>
              </span>
           </div>
        </form>
     </div>
  </div>
</div>

<div class="jumbotron">
  <div class="container">
    <ul class="list-group">
      <li
        ng-repeat="result in results"
        class="list-group-item">
        {{result.name}}
      </li>
    </ul>
  </div>
</div>
