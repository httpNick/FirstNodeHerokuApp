<div class="page-header text-center">
  <h1>CS:GO Skin Pricing</h1>
</div>

<ul class="nav nav-tabs">
 <li ng-repeat="wep in weapons">
   <a href="#{{wep.name}}" data-toggle="tab" aria-expanded="false" ng-click="selectMessage(wep)">
     {{wep.name}}
   </a>
 </li>
</ul>

<div class="col-md-5 col-md-offset-3">
  <div id="myTabContent" class="tab-content">
     <div class="tab-pane fade" ng-repeat = "wep in weapons" id="{{wep.name}}">
     <div ng-repeat="skin in wep.skins">
       <a href="#/wep/{{wep.name}} | {{skin}}"><img ng-src="images/{{wep.name}}-{{skin}}.png" style="width:50px;height:50px" /></a>
       <span class="label label-danger"> {{skin}} </span>
       <span class="label label-success">  <strong>Field-Tested</strong>: Lowest: <strong>{{wep.skinData[skin]['Field-Tested'].lowest_price}}</strong> Median: <strong>{{wep.skinData[skin]['Field-Tested'].median_price}}</strong></span>
     </div>
     </div>
  </div>
</div>
