<div class="page-header">
  <h1>CS:GO Skin Pricing</h1>
</div>

<ul class="nav nav-tabs">
 <li ng-repeat="wep in weapons">
   <a href="#{{wep.weapon}}" data-toggle="tab" aria-expanded="false" ng-click="selectMessage(wep)">
     {{wep.weapon}}
   </a>
 </li>
</ul>

<div id="myTabContent" class="tab-content">
   <div class="tab-pane fade" ng-repeat = "wep in weapons" id="{{wep.weapon}}">
   <div ng-repeat="skin in wep.skins">
     <a href="#/wep/{{wep.weapon}} | {{skin}}"><img ng-src="images/{{wep.weapon}}-{{skin}}.png" style="width:50px;height:50px" /></a>
     <span class="label label-danger"> {{skin}} </span>
     <span class="label label-success">  <strong>Field-Tested</strong>: Lowest: <strong>{{wep.skinData[skin]['Field-Tested'].lowest_price}}</strong> Median: <strong>{{wep.skinData[skin]['Field-Tested'].median_price}}</strong></span>
   </div>
   </div>
</div>
